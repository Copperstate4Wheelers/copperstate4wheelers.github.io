---
import Subnav from "@components/Subnav.astro";
import ContentPageLayout from "@layouts/ContentPageLayout.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<ContentPageLayout title={title} wide>
  <Subnav
    title="Members"
    links={[
      ["/members", "Current Members"],
      ["/members/in-memory-of", "In Memory Of"],
    ]}
  />

  <slot />
</ContentPageLayout>

<style></style>
