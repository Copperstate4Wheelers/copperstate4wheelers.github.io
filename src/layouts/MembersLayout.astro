---
import Subnav from "@components/Subnav.astro";
import BaseLayout from "@layouts/BaseLayout.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<BaseLayout title={title}>
  <Subnav
    title="Members"
    links={[
      ["/members", "Current Members"],
      ["/members/in-memory-of", "In Memory Of"],
    ]}
  />

  <slot />
</BaseLayout>
